{
  "parser": "typescript-eslint-parser",
  "plugins": [
    "typescript",
    "prettier",
    "prettier/standard"
  ],
  "extends": [
    "standard",
    "standard-react",
    "prettier"
    ],
  "env": { 
    "browser": true
  },
  "rules": {
    "no-unused-vars": "off",   
    "react/prop-types": "off"
  }
}
